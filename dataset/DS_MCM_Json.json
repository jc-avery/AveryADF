{
	"name": "DS_MCM_Json",
	"properties": {
		"linkedServiceName": {
			"referenceName": "PolestarStorageAcc",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileType": {
				"type": "string"
			}
		},
		"folder": {
			"name": "MCM"
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@Concat(dataset().FileType, ' - x', formatDateTime(utcNow(), 'yyyy'),'-',formatDateTime(utcNow(), 'MM'),'-',formatDateTime(utcNow(), 'dd'), '.json')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(formatDateTime(utcNow(), 'yyyy'),'/',formatDateTime(utcNow(), 'MM'),'/',formatDateTime(utcNow(), 'dd') )",
					"type": "Expression"
				},
				"container": "mcm"
			}
		},
		"schema": {}
	}
}