{
	"name": "DS_Sona_Json_WeeklyHrs",
	"properties": {
		"linkedServiceName": {
			"referenceName": "PolestarStorageAcc",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string"
			},
			"DateString": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Sona/Initial API FIles"
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().FileName,dataset().DateString,'.json')",
					"type": "Expression"
				},
				"folderPath": "@concat(formatDateTime(utcNow(), 'yyyy'),'/',formatDateTime(utcNow(), 'MM'),'/',formatDateTime(utcNow(), 'dd') )",
				"container": "sona"
			}
		},
		"schema": {}
	}
}