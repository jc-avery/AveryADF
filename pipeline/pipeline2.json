{
	"name": "pipeline2",
	"properties": {
		"activities": [
			{
				"name": "Copy Results to Stage table",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['results'][0]['id']"
								},
								"sink": {
									"name": "ResultID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['passed']"
								},
								"sink": {
									"name": "Passed",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['grade']"
								},
								"sink": {
									"name": "Grade",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['score']"
								},
								"sink": {
									"name": "Score",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['completed_at']"
								},
								"sink": {
									"name": "CompletedAt",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['expires_at']"
								},
								"sink": {
									"name": "ExpiresAt",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['has_expired']"
								},
								"sink": {
									"name": "HasExpired",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['course']['id']"
								},
								"sink": {
									"name": "CourseID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "$['results'][0]['user']['id']"
								},
								"sink": {
									"name": "UserID",
									"type": "String"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "DS_YourHippo_Merged_results",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_AH2_TRANSFORM_DB",
						"type": "DatasetReference",
						"parameters": {
							"Schema": "YourHippo",
							"Table": "StageResults"
						}
					}
				]
			}
		],
		"folder": {
			"name": "YourHippo/Child Pipelines"
		},
		"annotations": []
	}
}